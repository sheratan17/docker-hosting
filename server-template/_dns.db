; BIND zonefile for hanakama.my.id

$TTL    7200
_dns.         IN      SOA     ns1._dns. admin._dns. (
                                        _soa      ; Serial
                                        7200            ; Refresh
                                        3600            ; Retry
                                        604800          ; Expire
                                        7200)           ; NegativeCacheTTL

@                       IN      NS      ns1._dns.
@                       IN      NS      ns2._dns.

_dns.         IN      A       _ipnamed
www                     IN      CNAME   _dns.
ns1                     IN      A       _ipnamed
ns2                     IN      A       _ipnamedd
