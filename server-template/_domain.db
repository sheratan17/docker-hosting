; BIND zonefile for _domain

$TTL    7200
_domain.         IN      SOA     ns1._dns. admin._dns. (
                                        _soa      ; Serial
                                        7200            ; Refresh
                                        3600            ; Retry
                                        604800          ; Expire
                                        7200)           ; NegativeCacheTTL

@                       IN      NS      ns1._dns.
@                       IN      NS      ns2._dns.

_domain.         IN      A       _servernginx
www                     IN      CNAME   _domain.
ns1                     IN      A       _ip_named
ns2                     IN      A       _ip_nameed
pma			IN	A	_servernginx
file			IN	A	_servernginx
www.pma			IN	CNAME	pma._domain.
www.file		IN	CNAME	file._domain.
